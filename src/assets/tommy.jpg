        //     tealiumObj = {};
        //     tealiumObj.event_name = THTealium.EVENT_SITE_INTERACTION;
        //     tealiumObj.site_interaction = "livestream:glp:click_cta:" + _helpers.division + ":" + _helpers.device + ":watch the livestream:20210316"
        //     PVHTealium.submitUtagLink(tealiumObj);
        // })




        function addSourceToVideo(element, src, type) {
            var source = document.createElement('source');

            source.src = src;
            source.type = type;

            element.appendChild(source);
        }


        // carousel
        var carousel,
            card,
            leftButton,
            rightButton,
            carouselWrapper,
            carouselWidth,
            cardWidth,
            cardHeight,
            cardStyle,
            cardMarginRight,
            cardCount,
            clickOffset,
            offset,
            maxX,
            xDown = null,
            yDown = null,
            _isMobile = $('html.mobile').length > 0;

        function setCarDimensions() {
            carousel = document.querySelector("[data-target='carousel']");
            card = carousel.querySelector("[data-target='card']");
            leftButton = document.querySelector("[data-action='slideLeft']");
            rightButton = document.querySelector("[data-action='slideRight']");
            carouselWrapper = document.querySelector('.explore-row');
            carouselWidth = carousel.offsetWidth;
            cardWidth = card.clientWidth;

            cardHeight = carousel.clientHeight;
            carouselWrapper.style.minHeight = cardHeight + 'px';
            cardHeight = carousel.clientHeight;
            carouselWrapper.style.minHeight = cardHeight + 'px';
            cardStyle = card.currentStyle || window.getComputedStyle(card);
            cardMarginRight = Number(cardStyle.marginRight.match(/\d+/g)[0]);

            // Count the number of total cards you have
            cardCount = carousel.querySelectorAll("[data-target='card']").length;

            clickOffset = _isMobile ? 1 : 4;
            offset = 0;
            maxX = -((cardWidth + cardMarginRight) * cardCount);
        }



        function transitionButtons(side, state) {
            if (!_isMobile) {
                var dirBtn = side === 'left' ? leftButton : rightButton,
                    visState = state || '';
                if (visState === 'show') {
                    dirBtn.style.visibility = 'visible'
                    dirBtn.style.opacity = '1'
                } else {
                    dirBtn.style.visibility = 'hidden'
                    dirBtn.style.opacity = '0'
                }
            } else {
                return false
            }
        }
        function setButtons() {

            if (cardCount > clickOffset && !_isMobile) {
                transitionButtons('right', 'show')
                // Add the click events
                leftButton.addEventListener("click", leftBtn, false)
                rightButton.addEventListener("click", rightBtn, false)
            } else {
                leftButton.remove()
                rightButton.remove()
            }

        }
        function leftBtn() {
            if (offset !== 0) {
                transitionButtons('right', 'show')
                clickOffset--
                offset += cardWidth + cardMarginRight;
                carousel.style.transform = 'translateX(' + offset + 'px)';
            }
            if (offset === 0) {
                transitionButtons('left', 'hide')
            } else {
                transitionButtons('left', 'show')
            }
        }
        function rightBtn() {
            if (clickOffset !== cardCount) {
                transitionButtons('left', 'show')
                clickOffset++
                offset -= cardWidth + cardMarginRight;
                carousel.style.transform = 'translateX(' + offset + 'px)';
            }
            if (cardCount === clickOffset) {
                transitionButtons('right', 'hide')
            } else {
                transitionButtons('right', 'show')
            }
        }

        function getTouches(evt) {
            return evt.touches ||             // browser API
                evt.originalEvent.touches; // jQuery
        }
        function handleTouchStart(evt) {
            var firstTouch = getTouches(evt)[0];
            xDown = firstTouch.clientX;
            yDown = firstTouch.clientY;
        };

        function handleTouchMove(evt) {
            if (!xDown || !yDown) {
                return;
            }

            var xUp = evt.touches[0].clientX;
            var yUp = evt.touches[0].clientY;

            var xDiff = xDown - xUp;
            var yDiff = yDown - yUp;

            if (Math.abs(xDiff) > Math.abs(yDiff)) {/*most significant*/
                if (xDiff > 0) {
                    /* left swipe */
                    rightBtn()
                } else {
                    /* right swipe */
                    leftBtn()


                }
            } 
            /* reset values */
            xDown = null;
            yDown = null;
        };

        // video
        // video must have ID of #hp_video

        function initVideo(){
            
            var heroVideo = document.getElementById("hp_video");
            if(heroVideo){
                var $muteBtn = $('.target-video-mute');
    
    
                if (_isMobile) {
                    addSourceToVideo(heroVideo, '//media.tommy.com/us/static/images/scheduled_marketing/2021/08/TH_TOMMYXROMEOHUNTE_mb-2.mp4', 'video/mp4');
                } else {
                    addSourceToVideo(heroVideo, '//media.tommy.com/us/static/images/scheduled_marketing/2021/08/TH_TOMMYXROMEOHUNTE_dt-2.mp4', 'video/mp4');
                }
    
    
                heroVideo.play();
    
    
                $muteBtn.on("click", function () {
                    if (heroVideo.muted) {
                        $muteBtn.removeClass('muted')
                    } else {
                        $muteBtn.addClass('muted')
                    }
                    heroVideo.muted = !heroVideo.muted;
                });
            }else {
                return false;
            }

        }


        $(window).on('resize', function () {
            setCarDimensions()
        })
        $(window).on("load", function () {
            setCarDimensions()
        });
        if ((navigator.userAgent.match(/Trident/) && !navigator.userAgent.match(/MSIE/))) {
            $(window).one('scroll', function () {
                setCarDimensions()
            });
        }
        $(window).one('scroll', function () {
                setCarDimensions()
        });
        initVideo()
        setCarDimensions()
        setButtons()
        if (_isMobile) {
            carouselWrapper.addEventListener('touchstart', handleTouchStart, false);
            carouselWrapper.addEventListener('touchmove', handleTouchMove, false);
        }


        // var carouselWrapperMobile = document.querySelector('.explore-row');



    })






</script>
