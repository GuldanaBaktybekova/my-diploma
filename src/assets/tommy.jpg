<script>






    $(function () {


        // tracking
        /*TAGGING*/
        var _helpers = {
            device: $('html.mobile').length > 0 ? "mobile" : "desktop",
            icidLog: {},
            promo: !1
        };
        window._glpDebugger = _helpers.icidLog;

        var $_section = $('section.tagging');
        $_section.each(function (i) {
            var $_this = $(this),
                $_anchors = $_this.find('a'),
                _data = $_this.data('sectionInfo'),
                _name = _data.name,
                _date = _data.date;

            if (_name.indexOf('promo') >= 0) { _helpers.promo = 1 }

            tag($_anchors, _name, _date, i);
        });

        function tag(links, heading, date, count) {
            var _a = links || "",
                _seq = count + 1 || "",
                _section = heading || "",
                _date = date || "",
                _temp,
                _label,
                _slide = null,
                $_this;


            if (_helpers.promo) { _seq-- }
            if (_section.indexOf('promo') >= 0) {
                _seq = "0"
            }
            _glpDebugger["section_" + _seq] = {
                "section": _section,
                "date": _date,
                "links": []
            }
            $(_a).each(function (seq) {
                $_this = $(this);
                _temp = $(this).attr('href');
                _label = $_this.data('icidLabel').replace(/[^\w\s\-]/gi, '').toLowerCase().split(' ').join('_');
                if (_temp.indexOf('#') > 0) {
                    var _hash = '#' + _temp.split('#')[1];
                    _temp = _temp.split('#')[0];
                }
                if (_section.indexOf('carousel') >= 0) {
                    // $_this.closest('.mySlide').index()
                    _slide = $_this.closest('.mySlide').index() + 1
                }


var _fullUrl = _temp + '?icid=' + _seq + '.' + (seq + 1) + '-hp-' + _label + '-' + _section + '-' + _helpers.device + '-' + _date + (_slide ? '-slide_' + _slide : "") + (typeof _hash !== "undefined" ? _hash : '');


                $_this.attr('href', _fullUrl);

                _glpDebugger["section_" + _seq].links.push(_fullUrl)

            });
        };
        // var $interactionParent = $('.site-interaction')
        // $interactionParent.each(function () {
        //     var $this = $(this)
        //     _dataCampName = $this.data('interactionName')
        //     $click = $(this).find('a')
        //     $click.click(function () {
        //         var campName = _dataCampName,
