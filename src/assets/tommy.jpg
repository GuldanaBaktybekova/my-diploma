        //         tealiumObj.event_name = THTealium.EVENT_SITE_INTERACTION;
        //         tealiumObj.site_interaction = "[" + campName + "] click: summer VR: " + _helpers.division + " : " + _helpers.device
        //         PVHTealium.submitUtagLink(tealiumObj);
        //     })
