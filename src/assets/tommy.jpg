<!-- Newsletter signup validation -->
<script>
    $(document).ready(function () {

        function validateForm(targetForm) {
            var $tForm = targetForm
            nlSrcCode = $tForm.data('sourceCode')
            email_error_message = 'Please enter a valid email address.',
                post_error_message = 'There has been an error. Please try again later.',
                check_error_message = 'Please select the checkbox to sign up for newsletter.',
                emailUsed_error_message = 'This email address has already been added to the email list. Please register for a full Hilfiger Club account.',
                success_message = 'Thank you for signing up!',
                $emailInput = $tForm.find('input.nl-email-input'),
                email = $emailInput.val(),
                encEmail = encodeURIComponent(email),
                isEmailValid = validateEmail(email),
                $checkBox = $tForm.find('input.subscribe-checkbox-input'),
                checkBoxChecked = $checkBox.is(':checked'),
                form_url = '/webapp/wcs/stores/servlet/en/swim/AjaxSubscribeNewsletter',
                form_data = 'storeId=10151&catalogId=10551&email=' + encEmail + '&requesttype=ajax&sourceCode=' + nlSrcCode,
                $error_p = $tForm.find('p.error'),
                $nlSignup = $tForm.find('div.newsletter-banner'),
                $nlSuccess = $tForm.find('div.newsletter-signup-success');

            $error_p.text('');
            $emailInput.removeClass('input_error');
            $checkBox.removeClass('input_error');


            if (!isEmailValid) {
                $error_p.text(email_error_message);
                $emailInput.addClass('input_error');
                return false;
            } else if (isEmailValid && !checkBoxChecked) {
                $error_p.text(check_error_message);
                $checkBox.addClass('input_error');
                return false;
            }
            var debugNewsletter = []
            window.debugNewsletter = debugNewsletter;
            $.ajax({
                type: "POST",
                url: form_url,
                data: form_data,
                success: function (data) {


                    var respData = data;
                    var respTemp1 = respData.substring(2);
                    var respTemp2 = respTemp1.substring(0, respTemp1.length - 2);
                    var responseJson = JSON.parse(respTemp2);
                    debugNewsletter.push(responseJson)

                    if (responseJson.response === "failed") {
                        $error_p.text(post_error_message);
                        PVHTealium.submitNewsletterFailure(responseJson)
                    }
                    else if (!responseJson.alreadySignedUpForNewsletter) {
                        $nlSuccess.removeClass('hidden');
                        $nlSignup.addClass('hidden');
                        PVHTealium.submitNewsletterSuccess(responseJson)
                    }
                    else {
                        $error_p.text(emailUsed_error_message);
                        $emailInput.addClass('input_error');
                    }

                },
                error: function () {
                    $error_p.text(post_error_message);
                    PVHTealium.submitNewsletterFailure(responseJson)

                }
            });

        };
        function validateEmail(email) {
            var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
            return re.test(email);
        };

        $('.nl-email-submit').click(function (e) {
            var $parentWrap = $(e.target).closest('.nl-sign--wrap')
            // console.log($parentWrap.find('input.nl-email-input').val())
            validateForm($parentWrap);
        });
        document.getElementById("footer_year").innerHTML = new Date().getFullYear();

    });
</script>
