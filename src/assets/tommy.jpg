    <script>
    
    //Used for Shopping bag and Checkout  to handle when focus is lost because a dojo refreshArea updated.
    //Elements that cause a refresh to occur (QTY, Gift options, etc.) must each have a unique ID.
    var PVHFocusManager =
    {
        construct:function() {},
        
        init:function()
        {
            PVHFocusManager._addFocusListener();
        },
        
        finalize:function() {},
        
        //Called (from dojo.js setInnerHTML) to restore focus in case it was lost because a refreshArea was updated.
        restoreFocus:function()
        {
            PVHFocusManager._addFocusListener(); //Re-add listener to any elements that were replaced
            
            if(PVHFocusManager._currentFocusId && !U.hasFocus(PVHFocusManager._$currentFocus)) //Have a current id, but focus isn't still set to _$currentFocus, so restore it.
            {
                PVHFocusManager._$currentFocus=$(U.toId(PVHFocusManager._currentFocusId));
                PVHFocusManager._$currentFocus.focus();
            }
        },
        
        //Called (from dojo.js setInnerHTML) to capture the current focus before the HTML is replaced.
        setCurrentFocus:function()
        {
            if(PVHFocusManager._$currentFocus) PVHFocusManager._currentFocusId=PVHFocusManager._$currentFocus.attr(U.ID);
            else PVHFocusManager._currentFocusId=null;
        },
        
        _addFocusListener:function()
        {
            if(U.isShoppingBagPage() || U.isCheckout())
            {
                var ELEM_WITH_ID_SELECTOR='*[id]:not(.'+PVHFocusManager.FOCUS_LISTENER_CLASS+')';
                $(ELEM_WITH_ID_SELECTOR).addClass(PVHFocusManager.FOCUS_LISTENER_CLASS).focus(function()
                {
                    PVHFocusManager._$currentFocus=$(this);
                });
            }
        },
        
        _$currentFocus:null,
        _currentFocusId:null,
        FOCUS_LISTENER_CLASS:'focusListener'
    };
    
    
    </script>
    
